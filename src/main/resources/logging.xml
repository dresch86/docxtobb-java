<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="DocxToBB" packages="">
  <Appenders>
    <Console name="RootToConsole" target="SYSTEM_OUT">
        <PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss} %-5level root - %msg%n" />
    </Console>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss} %-5level %logger - %msg%n" />
    </Console>
    <File name="DocxToBBMainLog" fileName="${sys:java.io.tmpdir}/docxtobb.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Logger name="org.ose.docxtobb" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Root level="error">
      <AppenderRef ref="RootToConsole"/>
    </Root>
  </Loggers>
</Configuration>